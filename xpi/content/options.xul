				<menuitem value="false"	 label="&Ctr_tabsontop2;"/>
				<menuitem value="false2" label="&Ctr_tabsontop3;"/>
				<label value=" "/>
				<label value="&Ctr_tabsontopI1;" class="ctraddon_font1"/>
				<label value="#main-window, #navigator-toolbox, #titlebar, #nav-bar,"/>
				<label value="#toolbar-menubar, #TabsToolbar, #ctraddon_extra-bar,"/>
				<label value="#PersonalToolbar, #ctraddon_addon-bar"/>
			</menupopup>
		</menulist>

		<grid>
		  <rows>
			<row>
				<label value="&Ctr_tabminwidth;"/>
